{
  "expo": {
    "name": "Rocket-Elevators-Mobile",
    "slug": "Rocket-Elevators-Mobile",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./app/assets/images/icon.png",
    "splash": {
      "image": "./app/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
